<h3>Song Edit Form</h3>
<%= form_for(@song) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, value: @song.name %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_id, value: @song.artist_id %>
  <%= f.label :genre_name %>
  <%= f.text_field :genre_id, value: @song.genre_id %>
  <%= f.submit %>
<% end %>